{
  "name": "capstone-project-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "cd client && npm install",
    "build": "cd client && npm run build",
    "test:watch": "cd client && npm run test:watch",
    "test": "npm run lint && npm run prettier && cd client && npm test",
    "lint": "eslint . --ext .js",
    "prettify": "prettier --write .",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,md,mdx,html,css,json}\"",
    "server": "env-cmd nodemon server.js ",
    "start": "env-cmd nodemon server.js"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "axios": "^0.21.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "husky": "^4.3.0",
    "jade": "~1.11.0",
    "lint-staged": "^10.5.2",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.7",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.6",
    "prettier": "2.2.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
